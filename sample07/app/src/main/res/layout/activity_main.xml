<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


    <TableLayout
            android:layout_width="420dp"
            android:layout_height="588dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="128dp" android:layout_marginStart="88dp">

        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
            <TextView android:text="국어" android:textSize="30sp" android:padding="10dp" />
            <TextView android:text="수학" android:textSize="30sp" android:padding="10dp" />
            <TextView android:text="영어" android:textSize="30sp" android:padding="10dp" />
        </TableRow>

        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">
            <TextView android:text="80" android:textSize="30sp" android:padding="10dp" />
            <TextView android:text="90" android:textSize="30sp" android:padding="10dp" />
            <TextView android:text="100" android:textSize="30sp" android:padding="10dp" />
        </TableRow>
    </TableLayout>
</androidx.constraintlayout.widget.ConstraintLayout>