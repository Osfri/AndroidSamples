<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".InsertActivity">

    <Button
            android:text="Camera"
            android:layout_width="198dp"
            android:layout_height="87dp" android:id="@+id/cameraBtn"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.039" app:layout_constraintVertical_bias="0.036"/>
    <Button
            android:text="Insert"
            android:layout_width="198dp"
            android:layout_height="87dp" android:id="@+id/insertBtn"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.96" app:layout_constraintVertical_bias="0.036"/>
    <ImageView
            android:layout_width="421dp"
            android:layout_height="328dp" tools:srcCompat="@tools:sample/avatars" android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
    />
    <EditText
            android:layout_width="462dp"
            android:layout_height="60dp"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/editText1"
            tools:ignore="SpeakableTextPresentCheck"
            app:layout_constraintVertical_bias="0.797"/>
    <TextView
            android:layout_width="459dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_height="59dp" android:id="@+id/textView"
            app:layout_constraintVertical_bias="0.869" android:textSize="24sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>